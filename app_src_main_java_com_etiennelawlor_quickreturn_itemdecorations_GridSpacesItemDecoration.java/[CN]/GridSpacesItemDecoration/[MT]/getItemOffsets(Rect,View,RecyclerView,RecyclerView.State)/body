{
  if (parent.getChildPosition(view) == 0 || parent.getChildPosition(view) == 1) {
    outRect.top=space;
  }
  if (parent.getChildPosition(view) % 2 == 0) {
    outRect.left=space;
    outRect.right=space / 2;
  }
 else {
    outRect.left=space / 2;
    outRect.right=space;
  }
  outRect.bottom=space;
}
