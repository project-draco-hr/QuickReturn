{
  mQuickReturnType=quickReturnType;
switch (mQuickReturnType) {
case HEADER:
    mHeader=headerView;
  mMinHeaderTranslation=headerTranslation;
break;
case FOOTER:
mFooter=footerView;
mMinFooterTranslation=footerTranslation;
case BOTH:
mHeader=headerView;
mMinHeaderTranslation=headerTranslation;
mFooter=footerView;
mMinFooterTranslation=footerTranslation;
default :
break;
}
}
